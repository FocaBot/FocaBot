{
  "name": "focabot",
  "version": "2.0.0-alpha1",
  "description": "Music with Seals!",
  "main": "cli-bootstrap.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "start": "node build",
    "build": "tsc",
    "build-bin": "tsc && pkg --out-path bin . -t node12-linux-x64,node12-macos-x64,node12-win-x64,node12-win-x86",
    "build-arm": "tsc && pkg --out-path bin . -t node12-linux-armv7",
    "dev": "cross-env TS_NODE_FILES=true ts-node src",
    "locale-gen": "node utils/generateLocaleTypings.js"
  },
  "bin": {
    "focabot": "cli-bootstrap.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FocaBot/FocaBot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "music",
    "focabot"
  ],
  "author": "TheBITLINK aka BIT <me@thebitlink.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FocaBot/FocaBot/issues"
  },
  "homepage": "https://github.com/FocaBot/FocaBot#README",
  "dependencies": {
    "axios": "^0.18.0",
    "azarasi": "github:FocaBot/Azarasi#next",
    "chance": "^1.0.4",
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.19.0",
    "discord.js": "^11.4.0",
    "dotenv": "^2.0.0",
    "immutable": "^4.0.0-rc.12",
    "inquirer": "^3.1.0",
    "mathjs": "^5.1.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.14.1",
    "mz": "^2.7.0",
    "stream-throttle": "^0.1.3",
    "ytdl-getinfo": "^1.0.5"
  },
  "devDependencies": {
    "@types/chance": "^1.0.1",
    "@types/js-yaml": "^3.11.2",
    "@types/mathjs": "^4.4.1",
    "@types/mz": "^0.0.32",
    "@types/node": "^10.5.3",
    "@types/stream-throttle": "^0.1.0",
    "cross-env": "^5.2.0",
    "pkg": "^4.3.7",
    "ts-node": "^7.0.0",
    "typescript": "^3.4.0-rc"
  },
  "pkg": {
    "scripts": [
      "cli-bootstrap.js",
      "build/**/*.js"
    ],
    "assets": [
      "locales/**/*",
      "focabot.example.yml",
      "package.json"
    ]
  },
  "types": "./src/focabot.d.ts"
}
